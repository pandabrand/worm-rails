<p id="notice"><%= notice %></p>

<h1>Registered Component Htmls</h1>

<table>
  <thead>
    <tr>
      <th>Text data</th>
      <th>Date created</th>
      <th>Date modified</th>
      <th>Display name</th>
      <th>Fk component</th>
      <th>Name</th>
      <th>Sort order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registered_component_htmls.each do |registered_component_html| %>
      <tr>
        <td><%= registered_component_html.text_data %></td>
        <td><%= registered_component_html.date_created %></td>
        <td><%= registered_component_html.date_modified %></td>
        <td><%= registered_component_html.display_name %></td>
        <td><%= registered_component_html.fk_component %></td>
        <td><%= registered_component_html.name %></td>
        <td><%= registered_component_html.sort_order %></td>
        <td><%= link_to 'Show', registered_component_html %></td>
        <td><%= link_to 'Edit', edit_registered_component_html_path(registered_component_html) %></td>
        <td><%= link_to 'Destroy', registered_component_html, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registered Component Html', new_registered_component_html_path %>
