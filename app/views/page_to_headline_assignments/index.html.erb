<p id="notice"><%= notice %></p>

<h1>Page To Headline Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Headline sort order</th>
      <th>Cpkfk headline</th>
      <th>Cpkfk page content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @page_to_headline_assignments.each do |page_to_headline_assignment| %>
      <tr>
        <td><%= page_to_headline_assignment.headline_sort_order %></td>
        <td><%= page_to_headline_assignment.cpkfk_headline %></td>
        <td><%= page_to_headline_assignment.cpkfk_page_content %></td>
        <td><%= link_to 'Show', page_to_headline_assignment %></td>
        <td><%= link_to 'Edit', edit_page_to_headline_assignment_path(page_to_headline_assignment) %></td>
        <td><%= link_to 'Destroy', page_to_headline_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Page To Headline Assignment', new_page_to_headline_assignment_path %>
