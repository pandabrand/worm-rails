<p id="notice"><%= notice %></p>

<h1>Cdg Functions</h1>

<table>
  <thead>
    <tr>
      <th>Sort order</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cdg_functions.each do |cdg_function| %>
      <tr>
        <td><%= cdg_function.sort_order %></td>
        <td><%= cdg_function.name %></td>
        <td><%= link_to 'Show', cdg_function %></td>
        <td><%= link_to 'Edit', edit_cdg_function_path(cdg_function) %></td>
        <td><%= link_to 'Destroy', cdg_function, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cdg Function', new_cdg_function_path %>
