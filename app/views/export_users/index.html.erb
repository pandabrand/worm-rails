<p id="notice"><%= notice %></p>

<h1>Export Users</h1>

<table>
  <thead>
    <tr>
      <th>Emp</th>
      <th>Branch</th>
      <th>Region</th>
      <th>Last name</th>
      <th>First name</th>
      <th>Job title</th>
      <th>Email address</th>
      <th>Fk user</th>
      <th>Financial advisor</th>
      <th>Area</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @export_users.each do |export_user| %>
      <tr>
        <td><%= export_user.emp %></td>
        <td><%= export_user.branch %></td>
        <td><%= export_user.region %></td>
        <td><%= export_user.last_name %></td>
        <td><%= export_user.first_name %></td>
        <td><%= export_user.job_title %></td>
        <td><%= export_user.email_address %></td>
        <td><%= export_user.fk_user %></td>
        <td><%= export_user.financial_advisor %></td>
        <td><%= export_user.area %></td>
        <td><%= link_to 'Show', export_user %></td>
        <td><%= link_to 'Edit', edit_export_user_path(export_user) %></td>
        <td><%= link_to 'Destroy', export_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Export User', new_export_user_path %>
