<p id="notice"><%= notice %></p>

<h1>Cdg Users</h1>

<%= will_paginate @cdg_users %>
<table>
  <thead>
    <tr>
      <th>Date created</th>
      <th>Date modified</th>
      <th>Password</th>
      <th>Username</th>
      <th>Is active</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Date last logged in</th>
      <th>Skip login profile page</th>
      <th>New login</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cdg_users.each do |cdg_user| %>
      <tr>
        <td><%= cdg_user.date_created %></td>
        <td><%= cdg_user.date_modified %></td>
        <td><%= cdg_user.password %></td>
        <td><%= cdg_user.username %></td>
        <td><%= cdg_user.is_active %></td>
        <td><%= cdg_user.lab_user_info.name_last %></td>
        <td><%= cdg_user.lab_user_info.name_first %></td>
        <td><%= cdg_user.date_last_logged_in %></td>
        <td><%= cdg_user.skip_login_profile_page %></td>
        <td><%= cdg_user.new_login %></td>
        <td><%= link_to 'Show', cdg_user %></td>
        <td><%= link_to 'Edit', edit_cdg_user_path(cdg_user) %></td>
        <td><%= link_to 'Destroy', cdg_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cdg User', new_cdg_user_path %>
