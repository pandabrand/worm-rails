<p id="notice"><%= notice %></p>

<h1>Import New Branches</h1>

<table>
  <thead>
    <tr>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>District area</th>
      <th>Name</th>
      <th>State</th>
      <th>Zip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @import_new_branches.each do |import_new_branch| %>
      <tr>
        <td><%= import_new_branch.address1 %></td>
        <td><%= import_new_branch.address2 %></td>
        <td><%= import_new_branch.city %></td>
        <td><%= import_new_branch.district_area %></td>
        <td><%= import_new_branch.name %></td>
        <td><%= import_new_branch.state %></td>
        <td><%= import_new_branch.zip %></td>
        <td><%= link_to 'Show', import_new_branch %></td>
        <td><%= link_to 'Edit', edit_import_new_branch_path(import_new_branch) %></td>
        <td><%= link_to 'Destroy', import_new_branch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Import New Branch', new_import_new_branch_path %>
