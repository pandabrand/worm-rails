<p id="notice"><%= notice %></p>

<h1>Customvalues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Templateformfieldid</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customvalues.each do |customvalue| %>
      <tr>
        <td><%= customvalue.name %></td>
        <td><%= customvalue.templateFormFieldID %></td>
        <td><%= customvalue.value %></td>
        <td><%= link_to 'Show', customvalue %></td>
        <td><%= link_to 'Edit', edit_customvalue_path(customvalue) %></td>
        <td><%= link_to 'Destroy', customvalue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customvalue', new_customvalue_path %>
