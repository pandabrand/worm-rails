<p id="notice"><%= notice %></p>

<h1>Category To Categories</h1>

<table>
  <thead>
    <tr>
      <th>Cp fk copy category</th>
      <th>Cp fk destination category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @category_to_categories.each do |category_to_category| %>
      <tr>
        <td><%= category_to_category.cp_fk_copy_category %></td>
        <td><%= category_to_category.cp_fk_destination_category %></td>
        <td><%= link_to 'Show', category_to_category %></td>
        <td><%= link_to 'Edit', edit_category_to_category_path(category_to_category) %></td>
        <td><%= link_to 'Destroy', category_to_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category To Category', new_category_to_category_path %>
