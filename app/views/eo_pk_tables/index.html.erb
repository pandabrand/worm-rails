<p id="notice"><%= notice %></p>

<h1>Eo Pk Tables</h1>

<table>
  <thead>
    <tr>
      <th>Pk</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eo_pk_tables.each do |eo_pk_table| %>
      <tr>
        <td><%= eo_pk_table.PK %></td>
        <td><%= link_to 'Show', eo_pk_table %></td>
        <td><%= link_to 'Edit', edit_eo_pk_table_path(eo_pk_table) %></td>
        <td><%= link_to 'Destroy', eo_pk_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

