<p id="notice"><%= notice %></p>

<h1>Saved Publications</h1>

<table>
  <thead>
    <tr>
      <th>Contact name</th>
      <th>Contact phone</th>
      <th>Contact email address</th>
      <th>Date created</th>
      <th>Fk creator</th>
      <th>Fk publication type</th>
      <th>Is active</th>
      <th>Publication name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_publications.each do |saved_publication| %>
      <tr>
        <td><%= saved_publication.contact_name %></td>
        <td><%= saved_publication.contact_phone %></td>
        <td><%= saved_publication.contact_email_address %></td>
        <td><%= saved_publication.date_created %></td>
        <td><%= saved_publication.fk_creator %></td>
        <td><%= saved_publication.fk_publication_type %></td>
        <td><%= saved_publication.is_active %></td>
        <td><%= saved_publication.publication_name %></td>
        <td><%= link_to 'Show', saved_publication %></td>
        <td><%= link_to 'Edit', edit_saved_publication_path(saved_publication) %></td>
        <td><%= link_to 'Destroy', saved_publication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Saved Publication', new_saved_publication_path %>
