<p id="notice"><%= notice %></p>

<h1>Excel Valid Params</h1>

<table>
  <thead>
    <tr>
      <th>Is required</th>
      <th>Excel col name</th>
      <th>Excel col number</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @excel_valid_params.each do |excel_valid_param| %>
      <tr>
        <td><%= excel_valid_param.is_required %></td>
        <td><%= excel_valid_param.excel_col_name %></td>
        <td><%= excel_valid_param.excel_col_number %></td>
        <td><%= excel_valid_param.type %></td>
        <td><%= link_to 'Show', excel_valid_param %></td>
        <td><%= link_to 'Edit', edit_excel_valid_param_path(excel_valid_param) %></td>
        <td><%= link_to 'Destroy', excel_valid_param, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Excel Valid Param', new_excel_valid_param_path %>
