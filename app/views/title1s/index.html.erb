<p id="notice"><%= notice %></p>

<h1>Title1s</h1>

<table>
  <thead>
    <tr>
      <th>Is active</th>
      <th>Name</th>
      <th>Sort order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @title1s.each do |title1| %>
      <tr>
        <td><%= title1.is_active %></td>
        <td><%= title1.name %></td>
        <td><%= title1.sort_order %></td>
        <td><%= link_to 'Show', title1 %></td>
        <td><%= link_to 'Edit', edit_title1_path(title1) %></td>
        <td><%= link_to 'Destroy', title1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Title1', new_title1_path %>
