<p id="notice"><%= notice %></p>

<h1>Cdg Configurations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th>Is default</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cdg_configurations.each do |cdg_configuration| %>
      <tr>
        <td><%= cdg_configuration.name %></td>
        <td><%= cdg_configuration.value %></td>
        <td><%= cdg_configuration.is_default %></td>
        <td><%= link_to 'Show', cdg_configuration %></td>
        <td><%= link_to 'Edit', edit_cdg_configuration_path(cdg_configuration) %></td>
        <td><%= link_to 'Destroy', cdg_configuration, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cdg Configuration', new_cdg_configuration_path %>
