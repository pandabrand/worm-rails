<p id="notice"><%= notice %></p>

<h1>Import Financial Advisors</h1>

<table>
  <thead>
    <tr>
      <th>Branch</th>
      <th>Emp</th>
      <th>Financial advisor</th>
      <th>First name</th>
      <th>Job category</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @import_financial_advisors.each do |import_financial_advisor| %>
      <tr>
        <td><%= import_financial_advisor.branch %></td>
        <td><%= import_financial_advisor.emp %></td>
        <td><%= import_financial_advisor.financial_advisor %></td>
        <td><%= import_financial_advisor.first_name %></td>
        <td><%= import_financial_advisor.job_category %></td>
        <td><%= import_financial_advisor.last_name %></td>
        <td><%= link_to 'Show', import_financial_advisor %></td>
        <td><%= link_to 'Edit', edit_import_financial_advisor_path(import_financial_advisor) %></td>
        <td><%= link_to 'Destroy', import_financial_advisor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

