<p id="notice"><%= notice %></p>

<h1>Title3s</h1>

<table>
  <thead>
    <tr>
      <th>Isactive</th>
      <th>Name</th>
      <th>Sort order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @title3s.each do |title3| %>
      <tr>
        <td><%= title3.isActive %></td>
        <td><%= title3.name %></td>
        <td><%= title3.sort_order %></td>
        <td><%= link_to 'Show', title3 %></td>
        <td><%= link_to 'Edit', edit_title3_path(title3) %></td>
        <td><%= link_to 'Destroy', title3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Title3', new_title3_path %>
